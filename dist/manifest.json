{
  "manifest_version": 2,
  "name": "GitHub Wrangler",
  "description": "Manage issues and pull requests across multiple repos.",
  "version": "1.0",
  "background": {
    "scripts": ["GitHubIssueTracker.js", "main.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "GitHub Wrangler"
  },
  "options_page": "options.html",
  "permissions": ["alarms", "storage", "tabs"]
}
